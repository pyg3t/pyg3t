# Translated string
#: helloworld.c:42
msgid "Hello world!"
msgstr "Hej verden!"

# Fuzzy
#: fuzzy.c:42
#, fuzzy
msgid "Delete contents of disk"
msgstr "Slet ikke diskens indhold"

# Plurals partially translated, fuzzy
#: plurals.c:12
#, c-format, fuzzy
msgid "%d fatal error."
msgid_plural "%d fatal errors."
msgstr[0] "%d fatal fejl."
msgstr[1] ""

# Plurals translated, non-ASCII, repeated word in translation
#: lawyers.c:1
#, c-format
msgid ""
"It was so cold last winter that I saw %d lawyer with his hands in his own "
"pockets."
msgid_plural ""
"It was so cold last winter that I saw %d lawyers with their hands in their "
"own pockets."
msgstr[0] ""
"Det var så koldt sidste vinter, at jeg så %d advokat med hænderne i i sine "
"egne lommer."
msgstr[1] ""
"Det var så koldt sidste vinter, at jeg så %d advokater med hænderne i deres "
"egne lommer."

# Valid xml in msgstr
#: xml.c:1
msgid "<a href=\"http://nethack.org\">Nethack</a>"
msgstr "<a href=\"http://nethack.org\">Nethack</a>"

# Invalid xml in msgstr, bad leading case, bad trailing char
#: xml.c:2
msgid "See <a href=\"http://www.dansk-gruppen.dk\">Dansk-gruppen</a>"
msgstr "se <a href=\"http://www.dansk-gruppen.dk\">Dansk-gruppen<a/>!"

# Hotkey assignment, context char, trailing characters
#: hotkeys.c:2
msgid "action|Roll _tarball."
msgstr "Rul t_jærebolle"

# Hotkeys with the & character
#: hotkeys.c:3
msgid "Mail &daemon"
msgstr "Post&dæmon"

# Bad hotkey assignment, bad context char, tests multiple faults for poabc
# Also whitespace inconsistency
#: hotkeys.c:4
msgid "action|Write _emoticon"
msgstr "handling|Skriv følikon "

